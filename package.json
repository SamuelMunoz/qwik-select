{
  "name": "qwik-select",
  "version": "0.0.0-semantic-release",
  "description": "A select/autocomplete component for Qwik apps.",
  "homepage": "https://phongnn.github.io/qwik-select",
  "repository": {
    "url": "https://github.com/phongnn/qwik-select.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "node --inspect node_modules/vite/bin/vite.js --config vite.app.ts --mode ssr",
    "test": "vitest",
    "test.ci": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "cy": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:5173 \"npx cypress run\"",
    "typecheck": "tsc --incremental --noEmit",
    "build": "pnpm build.lib && pnpm build.types && pnpm build.styles",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "build.styles": "cp src/style.css dist",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "npx husky install",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^0.9.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.9.0",
    "@builder.io/qwik-city": "^0.0.111",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@faker-js/faker": "^7.5.0",
    "@testing-library/cypress": "^8.0.3",
    "@types/eslint": "8.4.6",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.12",
    "cypress": "10.9.0",
    "eslint": "8.24.0",
    "eslint-plugin-qwik": "0.9.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "node-fetch": "3.2.10",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.4",
    "vite": "3.1.4",
    "vite-tsconfig-paths": "^3.5.1",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": []
  }
}
